{
    "extends": "@vue/tsconfig/tsconfig.web.json",
    "include": [
        "env.d.ts",
        "src/**/*.d.ts",
        "src/**/*",
        "src/**/*.vue"
    ],
    "compilerOptions": {
        "composite": true,
        "baseUrl": ".",
        "declaration": true,
        "declarationDir": "types",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
    }
}
